<div class="icons-demo" id="icons">
  {% for item in items %}
    <div>
      {% include "@atoms/04-images/icons/_icon.twig" with {
        icon_name: item.value,
        icon_role: item.role,
        icon_title: item.title,
        icon_desc: item.desc,
        icon_decorative: item.decorative,
      } %}
      <p>{{ item.value }}</p>
    </div>
  {% endfor %}

  {% for item in topical_items %}
    <div>
      {% include "@atoms/04-images/icons/_icon.twig" with {
        icon_name: item.value,
        icon_role: item.role,
        icon_title: item.title,
        icon_desc: item.desc,
        icon_decorative: item.decorative,
        icon_filename: "icons",
      } %}
      <p>{{ item.value }}</p>
    </div>
  {% endfor %}
</div>
<br>

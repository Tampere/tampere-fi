{#
/**
 * Available variables:
 * - feedback_links__base_class - the base classname
 * - feedback_links__modifiers - array of modifiers to add to the base classname
 * - feedback_links__blockname - blockname prepended to the base classname
 * - feedback_links__items - array of links to display
 *
 * - feedback_links__list_item__modifiers - array of modifiers to add to the list items
 *
 * - feedback_links__link__modifiers - array of modifiers to add to the list item links
 */
#}
{% set feedback_links__base_class = "feedback-links" %}

<ul {{ bem(feedback_links__base_class, (feedback_links__modifiers|default([])), feedback_links__blockname) }}>
  {% block list_content %}
    {% for feedback_links__item in feedback_links__items %}
      {% include "@molecules/feedback-links/feedback-links-item.twig" with {
        "feedback_links__item_url": feedback_links__item.url,
        "is_external_link": feedback_links__item.is_external_link,
        "feedback_links__item_text": feedback_links__item.text,
        "feedback_links__item_icon": feedback_links__item.icon,
        "feedback_links__item_mobile_icon": feedback_links__item.mobile_icon,
        "feedback_links__icon_filename": feedback_links__icon_filename,
      } %}
    {% endfor %}
  {% endblock %}
</ul>

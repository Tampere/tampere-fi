{
  "name": "tampere",
  "version": "2.0.1",
  "description": "Storybook development + Webpack Build + Drupal 9+ theme for tampere.fi",
  "keywords": [
    "component library",
    "design system",
    "drupal",
    "pattern library",
    "storybook",
    "styleguide"
  ],
  "author": "Heikki Ylipaavalniemi <heikki@exove.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.13.13",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.1",
    "babel-preset-minify": "^0.5.0",
    "breakpoint-sass": "^2.7.1",
    "caniuse-lite": "^1.0.30001429",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "3.7.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.4",
    "hyphenopoly": "^4.9.0",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.18",
    "postcss-custom-properties": "^10.0.0",
    "postcss-loader": "^4.0.4",
    "sass": "^1.43.4",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "svg-sprite-loader": "^6.0.11",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.3.0"
  },
  "scripts": {
    "a11y": "npm run build-storybook && ./scripts/a11y.js -r",
    "lint": "eslint ./components && stylelint ./components/**/*.scss",
    "storybook": "start-storybook --ci -p 6006",
    "build-storybook": "npm run build && build-storybook -o .out",
    "deploy-storybook": "storybook-to-ghpages -o .out",
    "webpack": "webpack --watch --config ./webpack/webpack.dev.js",
    "build-webpack": "webpack -p --config ./webpack/webpack.prod.js --display-error-details",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "develop": "concurrently --raw \"npm run webpack\" \"npm run storybook\"",
    "postinstall": "npx -y patch-package && webpack --config ./webpack/webpack.prod.js"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-backgrounds": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addons": "6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "6.5.13",
    "@storybook/storybook-deployer": "^2.8.16",
    "@testing-library/react": "^10.2.1",
    "add-attributes-twig-extension": "^0.1.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.3.0",
    "bem-twig-extension": "^0.1.1",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "chalk": "^4.0.0",
    "commitizen": "^4.2.5",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.1",
    "open-cli": "^7.1.0",
    "pa11y": "^6.2.3",
    "prettier": "2.1.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "twig": "^1.15.3",
    "twig-drupal-filters": "^3.1.0",
    "twig-loader": "git+https://github.com/AmazeeLabs/twig-loader",
    "twig-testing-library": "^1.1.1",
    "yaml": "^1.10.0"
  },
  "engines": {
    "npm": ">=8.11.0"
  },
  "overrides": {
    "css-node-extract": {
      "postcss": "$postcss"
    },
    "css-selector-extract": {
      "postcss": "$postcss"
    },
    "glob-parent": "^5.1.2",
    "parse-url": "^8.1.0",
    "react": "$react",
    "react-dom": "$react-dom",
    "semver": "^7.5.2",
    "svg-baker": {
      "postcss": "$postcss"
    },
    "trim": "^0.0.3",
    "trim-newlines": "^3.0.1",
    "webpack": "$webpack"
  }
}
